{
    "manifest_version": 3,
    "name": "Feedless",
    "version": "0.1",
    "action" : {
        "default_popup": "popup/index.html"
    },
    "permissions": ["storage"],
    "web_accessible_resources": [
      {
        "resources": ["settings/index.html"],
        "matches": ["<all_urls>"]
      }
    ]
}